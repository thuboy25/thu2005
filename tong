#include <stdio.h>
#include <math.h>

int main() {
    int n, i;
    long long s = 0; 

    printf("Nhập giá trị n (n >= 10): ");
    scanf("%d", &n);


    if (n < 10) {
        printf("Giá trị n phải lớn hơn hoặc bằng 10.\n");
        return 0;
    }
    
    for (i = 1; i <= n; i++) {
       
        int num_digits = log10(i) + 1; 
        int nn = i * pow(10, num_digits) + i; 
        s += nn;
    }

    printf("Tổng S = %lld\n", s);

    return 0;
}
